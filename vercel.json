{
    "version": 2,
    "builds": [
      {
        "src": "portfolioapp/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/portfolioapp/wsgi.py"
      }
    ]
  }
  